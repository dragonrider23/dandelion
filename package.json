{
  "name": "dandelion-log",
  "description": "Web-based journal",
  "repository": "https://github.com/onesimus-systems/dandelion",
  "license": "GPL-3.0",
  "version": "6.2.0",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.1.0",
    "elm": "^0.19.0-bugfix6",
    "elm-webpack-loader": "^5.0.0",
    "eslint": "^5.10.0",
    "less": "^3.9.0",
    "less-plugin-clean-css": "^1.5.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sass-loader": "^7.1.0",
    "ts-loader": "^5.3.1",
    "typescript": "^3.2.2",
    "uglify-js": "^3.4.9",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.5"
  },
  "scripts": {
    "build": "yarn run build:js:prod && yarn run build:css",
    "build:dev": "yarn run build:js:dev && yarn run build:css",
    "build:js:dev": "env NODE_ENV=development webpack --mode development --config webpack.dev.js",
    "build:js:prod": "env NODE_ENV=production webpack --mode production --config webpack.prod.js",
    "build:css": "yarn run build:css:modern",
    "build:css:modern": "for file in public/src/themes/modern/*.less; do lessc --clean-css $file public/dist/assets/themes/modern/$(basename $file .less).css; done"
  },
  "dependencies": {
    "global": "^4.3.2"
  }
}
